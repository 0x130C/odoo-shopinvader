<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        
<record id="ir_attachment_media_view_form" model="ir.ui.view">
    <field name="model">ir.attachment.media</field>
    <field name="arch" type="xml">
        <form string="Media">
            <sheet>
                <label for="name" class="oe_edit_only"/>
                <h1>
                    <field name="name"/>
                </h1>
                <group name="data">
                    <field name="media_type"/>
                    <field name="datas" filename="datas_fname"/>
                    <field name="datas_fname" invisible="1"/>
                </group>
            </sheet>
        </form>
    </field>
</record>

<record id="ir_attachment_media_view_tree" model="ir.ui.view">
    <field name="model">ir.attachment.media</field>
    <field name="arch" type="xml">
        <tree string="Media">
            <field name="name"/>
            <field name="media_type"/>
            <field name="datas_fname"/>
        </tree>
    </field>
</record>

    </data>
</openerp>
